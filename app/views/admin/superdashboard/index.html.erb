<div class="row-fluid">
  <div class="span6">
    <div class="headerSmall headerAqua">
      <span class="headerText">Upcoming reminders</span>
    </div>
    <div class="conversation">
      <%= render 'admin/superdashboard/upcoming_reminders', object: @upcoming || 'There are no upcoming reminders' %>
    </div>
  </div>
  <div class="span6">
    <div class="headerSmall headerGreen">
      <span class="headerText">Sent reminders</span>
    </div>
    <div class="conversation">
      <%= render 'admin/superdashboard/sent_reminders', object: @conversations || 'There are no sent reminders' %>
    </div>
  </div>
</div>
<div class="row-fluid">
  <div class="span6">
    <div class="headerSmall headerGreen">
      <span class="headerText">Responses</span>
    </div>
    <div class="conversation">
      <%= render 'admin/superdashboard/responses' || 'There are no responses' %>
    </div>
  </div>
  <div class="span6">
    <div class="headerSmall headerRed">
      <span class="headerText">Undelivered reminders</span>
    </div>
    <div class="conversation">
      <%= render 'admin/superdashboard/undelivered' || 'There are no upcoming reminders' %>
    </div>
  </div>
</div>
<div class="row-fluid">
  <div class="span6">
    <div class="headerSmall headerAqua">
      <span class="headerText">Calls</span>
    </div>
    <div class="conversation">
      <%= render 'admin/superdashboard/calls' || 'There are no calls' %>
    </div>
  </div>
</div>